<%= form_with model: @post do |form| %>
  <%= form.label :supertitle %>
  <%= form.text_field :supertitle %>
  <%= form.label :secondtitle %>
  <%= form.text_field :secondtitle %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :body %>
  <%= form.text_area :body %>
  <%= form.label :important %>
  <%= form.check_box :important %>
  <%= form.label :photos %>
  <%= form.file_field :photos, multiple: true %>
  <%= form.submit %>
<% end %>
